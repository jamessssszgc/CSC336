X=zeros(13);
B=zeros(13,1);
B(2)=10;
B(8)=15;
B(10)=20;

a=sqrt(2)/2;
X(1,2)=1;
X(1,6)=-1;
X(2,3)=1;
X(3,1)=a;
X(3,4)=-1;
X(3,5)=-a;
X(4,1)=a;
